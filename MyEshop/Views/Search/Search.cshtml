@model IEnumerable<MyEshop.Core.DTOs.ProductsInSearchViewModel>
@inject IClientServices ClientServices;
@{
    var Categories = ClientServices.GetCategories();
    var WebsiteCategories = ClientServices.GetWebsiteCategories();
    var Brands = ClientServices.GetBrands();
    var Colors = ClientServices.GetColors();
    List<int> SelectedProductCategory = ViewBag.SelectedProductCategory;
    List<int> SelectedWebsiteCategory = ViewBag.SelectedWebsiteCategory;
    List<int> SelectedBrands = ViewBag.SelectedBrands;
    List<int> SelectedColors = ViewBag.SelectedColors;
    int EndPrice = ViewBag.EndPrice;
}

